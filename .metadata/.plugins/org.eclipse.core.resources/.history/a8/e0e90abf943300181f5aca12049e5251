package topics;

import java.util.Scanner;

public class tables {

	public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    System.out.println("Enter the number for its table=");
    int a = input.nextInt();
    for(int i= 1; i<=10; i++) {
    	int b= a * i;
    	System.out.println("a * "+i+" =" +b);
    	}
    

	}

}
